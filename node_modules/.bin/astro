#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kusum/Documents/projects/ludo/node_modules/.pnpm/astro@5.5.5_@netlify+blobs@10.5.0_@types+node@25.0.3_jiti@2.4.2_lightningcss@1.29.2_rol_396383428c90d3ff4631fb8c022c323d/node_modules/astro/node_modules:/Users/kusum/Documents/projects/ludo/node_modules/.pnpm/astro@5.5.5_@netlify+blobs@10.5.0_@types+node@25.0.3_jiti@2.4.2_lightningcss@1.29.2_rol_396383428c90d3ff4631fb8c022c323d/node_modules:/Users/kusum/Documents/projects/ludo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/kusum/Documents/projects/ludo/node_modules/.pnpm/astro@5.5.5_@netlify+blobs@10.5.0_@types+node@25.0.3_jiti@2.4.2_lightningcss@1.29.2_rol_396383428c90d3ff4631fb8c022c323d/node_modules/astro/node_modules:/Users/kusum/Documents/projects/ludo/node_modules/.pnpm/astro@5.5.5_@netlify+blobs@10.5.0_@types+node@25.0.3_jiti@2.4.2_lightningcss@1.29.2_rol_396383428c90d3ff4631fb8c022c323d/node_modules:/Users/kusum/Documents/projects/ludo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/astro@5.5.5_@netlify+blobs@10.5.0_@types+node@25.0.3_jiti@2.4.2_lightningcss@1.29.2_rol_396383428c90d3ff4631fb8c022c323d/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../.pnpm/astro@5.5.5_@netlify+blobs@10.5.0_@types+node@25.0.3_jiti@2.4.2_lightningcss@1.29.2_rol_396383428c90d3ff4631fb8c022c323d/node_modules/astro/astro.js" "$@"
fi
